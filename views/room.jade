doctype html
html
  head
    title mmhmmmmm
    link(rel='stylesheet/less', type='text/css', href='/stylesheets/style.less')
    link(href='http://fonts.googleapis.com/css?family=Julius+Sans+One|Open+Sans:400,300,700,600', rel='stylesheet', type='text/css')
    link(href='/stylesheets/font-awesome.min.css')
    script(type='text/javascript', src='/javascripts/jquery-1.10.1.min.js')
    script(type='text/javascript', src='/javascripts/less-1.6.1.min.js')
    script(type='text/javascript', src='/socket.io/socket.io.js')
    script(type='text/javascript', src='/javascripts/room.js')
  body
    div#logo
      img(src='/images/logo-black.png')
      div#tagline Is number one music app in eyeReturn bro.

    div#header
      h1#title #{roomName}
      
    div#content
      var#roomId(data-roomid='#{roomId}')
      h3 Currently playing : 
        var#currentTrackName Nothing
      h5
        var#listenerCount 0
        |  listeners; 
        var#skipCount 0
        |  skips; Rating: 
        var#rating.rating 0
      object#ytplayer(type='application/x-shockwave-flash', width='640', height='360', data='http://www.youtube.com/v/RHevGGYuAsw?enablejsapi=1&playerapiid=ytplayer&version=3')
        param(name='allowScriptAccess', value='always')
        param(name='allowFullScreen', value='false')
      div#controls
        button#skip Skip
        button.vote(data-val='up') Up
        button.vote(data-val='down') Down
      div#tracks
      div#trackHtml(style='display: none;')
        div.track(data-trackid='{trackId}')
          p {trackName}
          p Rating: 
            var.rating(data-trackid='{trackId}') {trackRating}
          button.vote(data-trackid='{trackId}', data-val='up') Up
          button.vote(data-trackid='{trackId}', data-val='down') Down
      div
        input#trackExtId(type='text', placeholder='Youtube ID')
        button#addTrack Add Track
      var#error
